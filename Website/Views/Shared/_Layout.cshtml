<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css/bootstrap.min.css")
    @Styles.Render("~/Content/css/dropdowns-enhancement.min.css")
    @Styles.Render("~/Content/css/Site.css")
    @Styles.Render("~/Content/HeaderStyle.css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/Content/js/jquery-2.1.4.min.js")
    @Scripts.Render("~/Content/js/bootstrap.min.js")
    @Scripts.Render("~/Content/js/dropdowns-enhancement.js")
    @Scripts.Render("~/Content/js/analytics.js")
</head>
<body>
    @RenderPage("~/Views/Shared/Header.cshtml")
    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
    @RenderPage("~/Views/Shared/Footer.cshtml")
</body>
</html>
